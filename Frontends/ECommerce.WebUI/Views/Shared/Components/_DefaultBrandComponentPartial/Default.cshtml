@model List<ResultBrandDto>

<div class="container py-4">
	<div class="row mt-3 pb-4">
		<div class="col text-center">
			<h2 class="font-weight-bold mb-0">İş Ortaklarımız</h2>
			<!-- <p class="lead text-4 pt-2 font-weight-normal">Porto comes with several elements options, it's easy to customize<br> and create the content of your website's pages.</p> -->
		</div>
	</div>
	<div class="row text-center">
		<div class="col">
			<div class="owl-carousel owl-theme" data-plugin-options="{'items': 6, 'autoplay': true, 'autoplayTimeout': 3000, 'dots': false, 'nav': false, 'margin': 20}">
				@foreach (var item in Model)
				{
					<div><img class="img-fluid" src="@item.ImageUrl" alt=""></div>
				}
			</div>
		</div>
	</div>
</div>